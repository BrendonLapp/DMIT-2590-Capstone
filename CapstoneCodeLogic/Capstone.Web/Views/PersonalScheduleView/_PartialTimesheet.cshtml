@model Capstone.Web.Models.PersonalSchedule.TimesheetPartialViewModel


@Html.ValidationSummary(false, "", new { @class = "text-danger" })
<div class="form-group">
    @Html.LabelFor(model => model.EventDate, htmlAttributes: new { @class = "control-label col-md-6" })
    <div class="col-md-10 removefloat">
        @{ Html.EnableClientValidation(false); }
        @Html.EditorFor(model => model.EventDate, new { htmlAttributes = new { id = "", @class = "form-control", autocomplete = "off" } })
        @{ Html.EnableClientValidation(true); }

    </div>
</div>

<div class="form-group">
    @Html.LabelFor(model => model.StartTime, htmlAttributes: new { @class = "control-label col-md-6" })
